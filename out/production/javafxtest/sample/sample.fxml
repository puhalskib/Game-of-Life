<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Canvas fx:id="canvasDisplay" height="800.0" width="800.0" />
      <HBox alignment="CENTER">
         <children>
            <Button fx:id="nextGenButton" mnemonicParsing="false" onAction="#onNextGenButtonClick" prefHeight="25.0" prefWidth="131.0" text="Next Generation" />
            <Button fx:id="resetButton" mnemonicParsing="false" onAction="#onResetClick" prefHeight="25.0" prefWidth="99.0" text="Clear" />
            <Button fx:id="startButton" mnemonicParsing="false" onAction="#onStartClick" text="Start" />
            <Button fx:id="stopButton" mnemonicParsing="false" onAction="#onStopClick" text="Stop" />
         </children>
      </HBox>
   </children>
</VBox>
